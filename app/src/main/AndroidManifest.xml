<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prabowo.promethee">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name=".SplashScreen"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Daftar"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            android:windowSoftInputMode="stateVisible|adjustPan" />
        <activity
            android:name=".Login"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            android:windowSoftInputMode="stateVisible|adjustPan" />
        <activity android:name=".PreferensiActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".ListKecamatanSet.SetKecamatanActivity" />
        <activity android:name=".MultiKriteriaActivity" />
        <activity android:name=".PrometheeRankingActivity" />
        <activity android:name=".ListKecamatanSet.EditKecamatanActivity" />
        <activity android:name=".ListviewHasilActivity" />
        <activity android:name=".HasilAkhirActivity"></activity>
    </application>

</manifest>